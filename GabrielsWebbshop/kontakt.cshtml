@{
    Layout = "~/Shared/_LayoutP.cshtml";
    Page.Title = "Kontakta Oss";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title></title>
    <meta />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="~/css/produkter.css" />
</head>
<body>
    <!--Formulär börjar-->
    <section id="contact" class="content-section text-center">
        <div class="contact-section">
            <div class="container">
                <h2>Kontakta Oss</h2>
                <p>Kontakta Oss idag och få ett svar från oss inom 24 timmar!</p>
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label for="exampleInputName2" class="placering">Namn</label>
                                <input type="text" class="form-control" id="exampleInputName2" placeholder="Jane Doe" tabindex="1" maxlength="20" required>
                            </div>
                            <div class="form-group">
                                <label class="placering" for="exampleInputEmail2">Email</label>
                                <input type="email" class="form-control" id="exampleInputEmail2" placeholder="jane.doe@example.com" tabindex="2" maxlength="50" required>
                            </div>
                            <div class="form-group ">
                                <label class="placering" for="exampleInputText">Meddelande</label>
                                <textarea class="form-control" placeholder="Ditt Meddelande" tabindex="3" maxlength="500" required></textarea>
                            </div>
                            <button type="submit" class="btn btn-default" data-submit="...Sending">Skicka</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--Formulär Slutar-->
    <!--Footer Börjar-->
    <!--Footer Börjar-->
