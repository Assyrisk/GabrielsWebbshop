@using WebMatrix;
@using System;
@using System.Collections.Generic;
@using System.Configuration;
@using System.Data.SqlClient;


<!--The code worked but on saturday all collapsed and stopt working, try this code on your computer otherwise delete the code to view the site-->



@{ using (SqlConnection con = new SqlConnection(ConfigurationManager.ConnectionStrings["LEDLights"].ConnectionString))
    {
        var db = Database.Open("LEDLights");
        var QueryString = "SELECT * FROM Information";
        var result = db.Query(QueryString);
        {
            foreach (var Information in result)
            {
                @Information.ID
                @Information.Name
                @Information.Email
            }
        }
    }
}


