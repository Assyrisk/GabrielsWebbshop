<!DOCTYPE html>
<html>
<head>

    @RenderSection("head", required: false)
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/css/cssforwebshop.css" />
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

    <title>startsida - @Page.Title</title>
    <style>
        #carouselExampleControls {
            max-width: 800px;
            max-height: 1300px;
            margin: auto;
            background: white;
            padding: 10px;
        }

        h1{

            text-align:center;
           color:white;
        }

        #myVideo {
            position: fixed;
            z-index: -1;
            right: 0;
            bottom: 0;
            min-width: 100%;
            min-height: 100%;
            filter: brightness(25%);
        }
    </style>
</head>
<body>
    <!-- Min header -->
    <div class="bs-example">
        <nav class="navbar navbar-expand-md navbar-dark bg-dark">
            <h1 class="navbar-brand">G&A <br/>LEDZ</h1>
            <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                <span class="navbar-toggler-icon"></span>
            </button>
            <br/>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <div class="navbar-nav">
                    <a href="~/hem.cshtml" class="nav-item nav-link ">Hem</a>
                    <a href="~/produkt.cshtml" class="nav-item nav-link">Produkter</a>
                    <a href="~/kontakt.cshtml" class="nav-item nav-link">Kontakta oss</a>
                    <a href="~/omoss.cshtml" class="nav-item nav-link">Om Oss</a>
                </div>
                <form class="form-inline ml-auto">
                    <input type="text" class="form-control mr-sm-2" placeholder="Search">
                    <button type="submit" class="btn btn-outline-light">Search</button>
                </form>
            </div>
        </nav>
    </div>
    <br />
    <h1>Välkomna kära kunder!</h1>
    <br />
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/Images/f7d0dd3a460b26ab17513f4194130ec3.jpg" class="d-block w-100" alt="..." width="800" height="400">
            </div>
            <div class="carousel-item">
                <img src="~/Images/behind-tv-1.jpg" class="d-block w-100" alt="..." width="800" height="400">
            </div>
            <div class="carousel-item">
                <img src="~/Images/DeskLightingBlue.jpg" class="d-block w-100" alt="..." width="800" height="400">
            </div>
            <div class="carousel-item">
                <img src="~/Images/a2e3ffddefe8372da0af1d44386a45ed.jpg" class="d-block w-100" alt="..." width="800" height="400">
            </div>
            <div class="carousel-item">
                <img src="~/Images/design-you-a-pc-setup.jpg" class="d-block w-100" alt="..." width="800" height="400">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>


    <video playsinline autoplay muted loop id="myVideo">
        <source src="~/Videos/videoplayback.mp4" type="video/mp4">
    </video>
    <br />



    <main>
        @RenderBody()
    </main>


</body>
</html>
